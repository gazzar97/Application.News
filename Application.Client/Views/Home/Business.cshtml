@{
}


<h1>@ViewBag.Title</h1>

<ul class="list-group">
    @foreach (var article in ViewBag.Articles.Item2)
    {
        <li class="list-group-item">
            <a href="@article.Url" target="_blank">@article.Title</a>
            (<em>@article.SourceName</em>, @article.publishedAt.ToString("dd/MM"))
        </li>
    }
</ul>